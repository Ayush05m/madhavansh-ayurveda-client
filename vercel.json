{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/services",
            "dest": "/services"
        },
        {
            "src": "/treatments",
            "dest": "/treatments"
        },
        {
            "src": "/doctors",
            "dest": "/doctors"
        },
        {
            "src": "/about",
            "dest": "/about"
        },
        {
            "src": "/track_consultation",
            "dest": "/track_consultation"
        },
        {
            "src": "/contact",
            "dest": "/contact"
        },
        {
            "src": "/auth",
            "dest": "/auth"
        },
        {
            "src": "/appointments",
            "dest": "/appointments"
        },
        {
            "src": "/history",
            "dest": "/history"
        },
        {
            "src": "/profile",
            "dest": "/profile"
        },
        {
            "src": "/book-consultation",
            "dest": "/book-consultation"
        },
        {
            "src": "/events",
            "dest": "/events"
        },
        {
            "src": "/[^.]+",
            "dest": "/index.html",
            "status": 200
        }
    ]
}